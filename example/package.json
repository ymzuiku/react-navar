{
  "name": "example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@nuage/classname": "^1.0.0",
    "@nuage/icon": "^1.0.7",
    "@types/jest": "24.0.15",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.5",
    "autoprefixer": "^9.6.1",
    "cssin": "^1.0.5",
    "immer": "^3.1.3",
    "postcss-cli": "^6.1.3",
    "querystring": "^0.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "3.5.3"
  },
  "scripts": {
    "build:css": "postcss tailwind.config.css -o src/tailwind.css",
    "build:css-prod": "NODE_ENV=production postcss tailwind.config.css -o src/tailwind.css",
    "start": "react-scripts start",
    "build": "npm run build:css-prod && react-scripts build && npm run build:css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lib": "tscu example/src/lib --outDir lib",
    "lint-ci": "lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-ci"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "git add",
      "tslint"
    ],
    "src/**/*.tsx": [
      "git add",
      "tslint"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "tailwindcss": "^1.0.5",
    "tscu": "^0.0.1",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0"
  }
}
